{
  "name": "alibabacloud/viapi-tool",
  "description": "Alibaba Cloud Viapiutils Tool Library for PHP",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Alibaba Cloud SDK",
      "email": "sdk-team@alibabacloud.com"
    }
  ],
  "require": {
    "php": ">5.5",
    "alibabacloud/tea": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "AlibabaCloud\\SDK\\ViapiTool\\": "src"
    }
  },
  "scripts": {
    "fixer": "php-cs-fixer fix ./"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "prefer-stable": true
}
